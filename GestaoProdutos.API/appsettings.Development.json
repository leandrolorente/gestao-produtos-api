{
  "ConnectionStrings": {
    "MongoDB": "mongodb://localhost:27017",
    "Redis": "localhost:6379"
  },
  "MongoDB": {
    "DatabaseName": "GestaoProdutosDB"
  },
  "Redis": {
    "ConnectionString": "localhost:6379",
    "InstanceName": "GestaoProdutos-Dev",
    "ConnectTimeout": 10,
    "SyncTimeout": 10,
    "ConnectRetry": 2,
    "DefaultTtlMinutes": 5,
    "CacheSettings": {
      "DashboardTtlMinutes": 1,
      "ProductListTtlMinutes": 2,
      "ClienteTtlMinutes": 5,
      "SessionTtlMinutes": 60,
      "ConfigTtlMinutes": 10,
      "ReportTtlMinutes": 30,
      "SearchTtlMinutes": 5,
      "AddressTtlHours": 1
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "GestaoProdutos.Application.Services.RedisCacheService": "Debug",
      "GestaoProdutos.Application.Services.HybridCacheService": "Debug",
      "GestaoProdutos.Application.Services.MemoryCacheService": "Information"
    }
  }
}
